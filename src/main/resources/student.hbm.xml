<?xml version="1.0" encoding="UTF-8"?>

 
<hibernate-mapping>
   <class name="com.jpa.hibernate.models.Student" table="students">
       <id name="id" type="int" column="id">
         <generator class="native"/>
      </id>
      <property name="name" column="student_name" type="string"/>
      <property name="roll" column="student_roll" type="int"/>
      <property name="course" column="student_course" type="string"/>
     <property name="address" column="student_address" type="string"/>
      <property name="age" column="student_age" type="int"/>
      
   </class>
</hibernate-mapping>

